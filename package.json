{
  "name": "starterkit",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config ./build/webpack/local.webpack.config.babel.js --env=localdev",
    "start": "webpack serve --open --config ./build/webpack/local.webpack.config.babel.js --env=localdev"
  },
  "author": "RLuis N",
  "dependencies": {
    "@popperjs/core": "^2.9.3",
    "@types/react": "^17.0.19",
    "@types/react-bootstrap": "^0.32.26",
    "@types/react-dom": "^17.0.9",
    "@types/react-router": "^5.1.16",
    "@types/react-router-dom": "^5.1.8",
    "bootstrap": "^5.1.0",
    "font-awesome": "^4.7.0",
    "history": "^5.0.1",
    "react": "^17.0.2",
    "react-bootstrap": "2.0.0-beta.5",
    "react-dom": "^17.0.2",
    "react-router": "6.0.0-beta.2",
    "react-router-dom": "6.0.0-beta.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/register": "^7.15.3",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "autoprefixer": "^10.3.4",
    "babel-loader": "^8.2.2",
    "core-js": "3",
    "css-loader": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^3.0.1",
    "html-webpack-plugin": "^5.3.2",
    "postcss": "^8.3.6",
    "postcss-loader": "^6.1.1",
    "postcss-preset-env": "^6.7.0",
    "sass": "1.32",
    "sass-loader": "^12.1.0",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.2.1",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.4.3",
    "typescript-eslint": "^0.0.1-alpha.0",
    "webpack": "5",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.0.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "globals": {
      "fetch": true
    },
    "root": true,
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2019,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "@typescript-eslint",
      "react",
      "react-hooks",
      "import"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": [
        "warn"
      ],
      "react/prop-types": "warn",
      "import/no-unresolved": "warn",
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    },
    "settings": {
      "react": {
        "pragma": "React",
        "version": "detect"
      },
      "import/parsers": {
        "@typescript-eslint/parser": [
          ".ts",
          ".tsx"
        ]
      },
      "import/resolver": {
        "node": {
          "extensions": [
            ".ts",
            ".tsx"
          ]
        },
        "typescript": {
          "extensions": [
            ".ts",
            ".tsx",
            ".js",
            ".jsx"
          ],
          "alwaysTryTypes": true
        }
      }
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          },
          "corejs": "3",
          "useBuiltIns": "usage"
        }
      ]
    ]
  }
}
